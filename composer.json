{
    "name": "kronos/oauth2-microsoft-graph",
    "description": "Microsoft Graph OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Maarten van Zanten",
            "email": "info@81square.nl",
            "homepage": "https://github.com/81square"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "microsoft",
        "microsoft graph",
        "azure"
    ],
    "require": {
        "php": ">=7.2.0",
        "league/oauth2-client": "^2.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.20",
        "mockery/mockery": "^1.3.1",
        "squizlabs/php_codesniffer": "~2.0",
        "vimeo/psalm": "4.23.0",
        "cyclonedx/cyclonedx-php-composer": "3.10.0"
    },
    "autoload": {
        "psr-4": {
            "EightyOneSquare\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EightyOneSquare\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "cyclonedx/cyclonedx-php-composer": true
        }
    }
}
